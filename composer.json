{
    "name": "biurad/event-manager",
    "type": "library",
    "description": "BiuradPHP Framework Event manager for listening and dispatching events",
    "keywords": ["events", "event dispatcher", "biuradphp", "php7"],
    "homepage": "https://www.biurad.com",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Divine Niiquaye Ibok",
            "email": "divineibok@gmail.com"
        },
        {
            "name": "Biurad Lap Community",
            "homepage": "https://biurad.com/contributors"
        }
    ],
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": { "BiuradPHP\\Event\\": "src/" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
	"autoload-dev": {
        "psr-4": {
            "BiuradPHP\\Event\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "suggest": {
        "biurad/dependency-injection": "For autowiring the dependency in container",
        "doctrine/annotations": "^For handling events listeners on annotations"
    }
}

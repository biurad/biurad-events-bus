{
    "name": "biurad/biurad-events-bus",
    "type": "library",
    "description": "Event manager for listening and dispatching events",
    "keywords": [
        "events",
        "event dispatcher",
        "biuradphp",
        "php7"
    ],
    "homepage": "https://www.biurad.com",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Divine Niiquaye Ibok",
            "email": "divineibok@gmail.com"
        },
        {
            "name": "Biurad Lap Community",
            "homepage": "https://biurad.com/contributors"
        }
    ],
    "require": {
        "php": "^7.2",
        "psr/log": "^1.1",
        "biurad/biurad-helpers": "^0.1",
        "symfony/event-dispatcher-contracts": "^1.1|^2"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.7"
    },
    "autoload": {
        "psr-4": {
            "BiuradPHP\\Events\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BiuradPHP\\Events\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "suggest": {
        "biurad/biurad-loader": "^For handling events listeners on annotations"
    }
}

{
    "name": "biurad/biurad-events-bus",
    "type": "library",
    "description": "Symfony Event manager for listening and dispatching events",
    "keywords": [
        "events",
        "event dispatcher",
        "biuradphp",
        "php7"
    ],
    "homepage": "https://www.biurad.com",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Divine Niiquaye Ibok",
            "email": "divineibok@gmail.com"
        },
        {
            "name": "Biurad Lap Community",
            "homepage": "https://biurad.com/contributors"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "biurad/biurad-helpers": "^0.1",
        "psr/container": "^1.0",
        "symfony/event-dispatcher": "^4.4|^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "BiuradPHP\\Events\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BiuradPHP\\Events\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "suggest": {
        "biurad/doctrine-annotation-bridge": "^For handling events listeners on annotations"
    }
}
